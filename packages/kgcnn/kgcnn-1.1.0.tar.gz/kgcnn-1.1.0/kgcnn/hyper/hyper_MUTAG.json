    "INorp": {
        "model": {
            "name": "INorp",
            "inputs": [
                {"shape": [null], "name": "node_attributes", "dtype": "float32", "ragged": true},
                {"shape": [null], "name": "edge_attributes", "dtype": "float32", "ragged": true},
                {"shape": [null, 2], "name": "edge_indices", "dtype": "int64", "ragged": true},
