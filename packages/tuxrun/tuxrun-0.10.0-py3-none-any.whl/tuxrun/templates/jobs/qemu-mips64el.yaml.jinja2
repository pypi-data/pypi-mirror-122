{% extends 'base-qemu.yaml.jinja2' %}

{% block context %}
context:
  arch: "mips64el"
  machine: "malta"
  no_kvm: true
  no_network: true
{% endblock %}

{% set arch = "mips64el" %}
{% set console = "ttyS0" %}
{% set rootfs_dev = "/dev/hda" %}
{% set rootfs_arg = "-drive file={rootfs},if=ide,format=raw" %}

