{% extends 'base-qemu.yaml.jinja2' %}

{% block context %}
context:
  arch: "sparc64"
  machine: "sun4u"
  no_kvm: true
  no_network: true
{% endblock %}

{% set arch = "sparc64" %}
{% set console = "ttyS0" %}
{% set rootfs_dev = "/dev/sda" %}
{% set rootfs_arg = "-drive file={rootfs},if=ide,format=raw" %}

