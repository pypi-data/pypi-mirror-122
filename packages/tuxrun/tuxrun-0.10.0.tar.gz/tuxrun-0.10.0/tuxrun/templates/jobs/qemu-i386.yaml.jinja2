{% extends 'base-qemu.yaml.jinja2' %}

{% block context %}
context:
  arch: "i386"
  cpu: "coreduo"
  machine: "q35"
  no_kvm: true
  no_network: true
  extra_options:
  - "-smp 2"
{% endblock %}

{% set arch = "i386" %}
{% set console = "ttyS0" %}
{% set rootfs_dev = "/dev/sda" %}
{% set rootfs_arg = "-drive file={rootfs},if=ide,format=raw" %}

