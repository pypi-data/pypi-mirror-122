{% extends 'base-qemu.yaml.jinja2' %}

{% block context %}
context:
  arch: "mips"
  cpu: "mips32r6-generic"
  machine: "malta"
  no_kvm: true
  no_network: true
{% endblock %}

{% set arch = "mips32" %}
{% set console = "ttyS0" %}
{% set rootfs_dev = "/dev/sda" %}
{% set rootfs_arg = "-drive file={rootfs},if=ide,format=raw" %}
