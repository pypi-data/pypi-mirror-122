#!/home/lukas/.virtualenvs/pylanguagetool/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pyLanguagetool==0.9.2','console_scripts','pylanguagetool'
__requires__ = 'pyLanguagetool==0.9.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pyLanguagetool==0.9.2', 'console_scripts', 'pylanguagetool')()
    )
